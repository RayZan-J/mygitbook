{
    "author": "zll",
    "description": "This is a sample book created by gitbook",
    "extension": null,
    "generator": "site",
    "isbn": "",
    "links": {
        "sharing": {
            "all": null,
            "facebook": null,
            "google": null,
            "twitter": null,
            "weibo": null
        },
        "sidebar": {}
    },
    "output": null,
    "pdf": {
        "fontSize": 12,
        "footerTemplate": null,
        "headerTemplate": null,
        "margin": {
            "bottom": 36,
            "left": 62,
            "right": 62,
            "top": 36
        },
        "pageNumbers": false,
        "paperSize": "a4"
    },
    "plugins": [
        "-lunr",
        "-search",
        "search-plus",
        "splitter",
        "anchor-navigation-ex",
        "-fontsettings",
        "-sharing",
        "sharing-plus",
        "baidu",
        "expandable-chapters",
        "theme-fexa",
        "multipart"
    ],
    "pluginsConfig": {
        "anchor-navigation-ex": {
            "associatedWithSummary": true,
            "float": {
                "floatIcon": "fa fa-navicon",
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right",
                "showLevelIcon": false
            },
            "mode": "float",
            "multipleH1": true,
            "pageTop": {
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right",
                "showLevelIcon": false
            },
            "printLog": false,
            "showGoTop": true,
            "showLevel": true
        },
        "baidu": {
            "token": "YOUR TOKEN"
        },
        "googledocs": {
            "frameborder": "10",
            "height": "500px",
            "noembed": "new window",
            "rm": "demo",
            "width": "100%"
        },
        "sharing": {
            "all": [
                "douban"
            ],
            "douban": true,
            "facebook": false,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": true,
            "qzone": true,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": true
        },
        "theme-fexa": {
            "favicon": "./favicon.ico",
            "logo": "./logo.png",
            "search-placeholder": "\u8f93\u5165\u5173\u952e\u5b57\u641c\u7d22"
        }
    },
